  "dependencies": {
    "@t3-oss/env-nextjs": "^0.6.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.264.0",
    "next": "^13.4.11",
    "nextra": "^2.10.0",
    "nextra-theme-docs": "^2.10.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "tailwind-merge": "^1.14.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/eslint": "^8.44.0",
    "@types/node": "^20.4.2",
    "@types/prettier": "^2.7.3",
    "@types/react": "^18.2.15",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "6.2.1",
    "@typescript-eslint/parser": "6.2.1",
    "autoprefixer": "^10.4.14",
    "chokidar": "^3.5.3",
    "eslint": "^8.45.0",
    "eslint-config-next": "^13.4.11",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "postcss": "^8.4.27",
    "prettier": "^3.0.0",
    "prettier-plugin-tailwindcss": "^0.4.1",
    "rimraf": "^5.0.1",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.6",
    "typescript": "^5.1.6"
  },
  "lint-staged": {
    "*.{html,js,ts,tsx,json,yml,css,scss}": "prettier --write",
    "*.{js,ts,tsx}": "eslint"
  }
}
