{
  "name": "pedaki.fr",
  "version": "0.1.0",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/PedakiApp/pedaki.fr.git"
  },
  "engines": {
    "node": ">=18.0.0 <19.0.0",
    "pnpm": ">=8.0.0"
  },
  "packageManager": "pnpm@8.6.11",
  "scripts": {
    "build": "next build",
    "dev": "next dev",
    "lint": "next lint",
    "start": "next start",
    "prepare": "husky install",
    "clean": "rimraf .next",
    "format:check": "prettier --check \"**/*.{ts,tsx,mdx}\" --cache",
